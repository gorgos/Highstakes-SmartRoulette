    "react": "^15.4.2",
    "react-async-script": "^0.9.1",
    "react-dom": "^15.4.2",
    "react-google-recaptcha": "^0.9.9",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-rotation": "^1.3.0",
    "react-spinning-wheel": "^1.1.6",
